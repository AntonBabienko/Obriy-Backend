PORT=8080
NODE_ENV=development

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-role-key

# Google Gemini API (1M токенів контексту! - отримайте ключ на https://aistudio.google.com/app/apikey)
# Система ротації: додайте декілька ключів для обходу лімітів Free Tier
GEMINI_API_KEY=your_gemini_api_key_here
# GEMINI_API_KEY_2=your_second_key_here
# GEMINI_API_KEY_3=your_third_key_here
# GEMINI_API_KEY_4=your_fourth_key_here
# GEMINI_API_KEY_5=your_fifth_key_here

# Groq API (deprecated - використовуємо Gemini)
GROQ_API_KEY=gsk_your_groq_api_key_here

# OpenAI API (ОПЦІОНАЛЬНО - не потрібен! Використовуємо локальні embeddings)
# OPENAI_API_KEY=sk_your_openai_key_here

# CORS
CORS_ORIGIN=http://localhost:5173

# AI Response Cache Configuration
# Enable/disable caching system (true/false)
CACHE_ENABLED=true

# Cache cleanup settings
CACHE_CLEANUP_DAYS=30
CACHE_CLEANUP_INTERVAL_HOURS=24

# Cache performance settings
CACHE_MAX_ENTRIES=10000
CACHE_MAX_SIZE_MB=500

# Debug cache operations (true/false)
DEBUG_CACHE=false
