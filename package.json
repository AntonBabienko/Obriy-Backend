{
    "name": "obriy-backend",
    "version": "1.0.0",
    "description": "Backend API for Obriy educational platform with Supabase",
    "main": "dist/server.js",
    "scripts": {
        "dev": "node --expose-gc --max-old-space-size=4096 node_modules/tsx/dist/cli.mjs src/server.ts",
        "dev:watch": "node --expose-gc --max-old-space-size=8192 node_modules/tsx/dist/cli.mjs watch src/server.ts",
        "build": "tsc",
        "start": "node --expose-gc dist/server.js",
        "lint": "eslint src --ext .ts",
        "test": "vitest",
        "test:run": "vitest run",
        "test:ui": "vitest --ui"
    },
    "keywords": [
        "education",
        "api",
        "express",
        "supabase",
        "ai"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@google/generative-ai": "^0.24.1",
        "@supabase/supabase-js": "^2.39.0",
        "@xenova/transformers": "^2.17.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "groq-sdk": "^0.37.0",
        "mammoth": "^1.6.0",
        "multer": "^1.4.5-lts.1",
        "openai": "^4.24.1",
        "pdf-parse": "^1.1.1",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.10.5",
        "@types/pdf-parse": "^1.1.4",
        "@vitest/ui": "^4.0.15",
        "fast-check": "^4.4.0",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3",
        "vitest": "^4.0.15"
    }
}